<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="thuan.dev.controller.CartController"
            prefHeight="400.0" prefWidth="600.0">

    <ImageView fx:id="cart_images" layoutX="14.0" layoutY="14.0" fitHeight="94.0" fitWidth="190.0"/>
    <Label fx:id="cart_name" layoutX="14.0" layoutY="118.0" text="Product Name"/>
    <Label fx:id="cart_price" layoutX="14.0" layoutY="146.0" text="Price"/>
    <Spinner fx:id="cart_spinner" layoutX="14.0" layoutY="174.0"/>
    <Button fx:id="cart_addBtn" layoutX="14.0" layoutY="202.0" text="Add to Cart" onAction="#addBtn"/>

</AnchorPane>
